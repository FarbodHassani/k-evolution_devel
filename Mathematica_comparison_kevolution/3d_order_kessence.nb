  "1175e3ad-6727-405b-9794-28b45f43fbdd"],
 "+2 ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "ij"], 
    SuperscriptBox["N", "i"], " ", 
    SuperscriptBox["dx", "j"]}], TraditionalForm]],ExpressionUUID->
  "0218bf4e-d351-479e-a3dd-e514fb2c14aa"],
