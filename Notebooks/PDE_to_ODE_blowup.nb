Cell[CellGroupData[{
Cell["Spherically symmetric case (1+1D nonlinear PDE)", "Section",
 CellChangeTimes->{{3.809679474265813*^9, 
  3.809679504527206*^9}},ExpressionUUID->"15c0a5e6-4478-457b-bf36-\
9c21d37107e6"],

Cell["As in the draft eq. (4.61), I write \[PartialD]lnH/\[PartialD]lna as", \
"Text",
 CellChangeTimes->{{3.809679514827734*^9, 
  3.809679568157173*^9}},ExpressionUUID->"8efb85a6-3bf0-4a73-8177-\
b2d04e8d4933"],

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"dlnHdlna", " ", "=", " ", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[CapitalOmega]m", " ", "+", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"3", "w"}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"1", "-", "\[CapitalOmega]m"}], ")"}], 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "3"}], "w", " ", "lna"}], "]"}]}]}], 
      RowBox[{
       RowBox[{"2", "\[CapitalOmega]m"}], " ", "+", " ", 
       RowBox[{"2", 
        RowBox[{"(", 
         RowBox[{"1", "-", "\[CapitalOmega]m"}], ")"}], 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "3"}], "w", " ", "lna"}], "]"}]}]}]]}]}], 
   ";"}]}]], "Input",
 CellChangeTimes->{{3.8096788283477716`*^9, 3.80967892734715*^9}, {
  3.850887945092527*^9, 3.850887965440065*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"187b863d-41f4-4023-879c-e0c9db6a3def"],

Cell["\<\
As an example I will put \[OpenCurlyDoubleQuote]nice\[CloseCurlyDoubleQuote] \
numbers, \[CapitalOmega]m = 3/10 and w = -5/6. We can solve the time \
evolution of the linear gravitational potential \[Psi]:\
\>", "Text",
 CellChangeTimes->{{3.809679594229014*^9, 
  3.80967970328369*^9}},ExpressionUUID->"c5c37f50-5bb0-479c-9e55-\
2756394a1e41"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"\[Psi]", "''"}], "[", "lna", "]"}], " ", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"3", "+", "dlnHdlna"}], ")"}], 
         RowBox[{
          RowBox[{"\[Psi]", "'"}], "[", "lna", "]"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", "-", 
           FractionBox[
            RowBox[{"3", "\[CapitalOmega]m"}], 
            RowBox[{
             RowBox[{"2", "\[CapitalOmega]m"}], " ", "+", " ", 
             RowBox[{"2", 
              RowBox[{"(", 
               RowBox[{"1", "-", "\[CapitalOmega]m"}], ")"}], 
              RowBox[{"Exp", "[", 
               RowBox[{
                RowBox[{"-", "3"}], "w", " ", "lna"}], "]"}]}]}]], "+", 
           "dlnHdlna"}], ")"}], 
         RowBox[{"\[Psi]", "[", "lna", "]"}]}]}], "\[Equal]", " ", "0"}], 
      ")"}], "/.", 
     RowBox[{"\[CapitalOmega]m", "\[Rule]", " ", 
      RowBox[{"3", "/", "10"}]}]}], "/.", 
    RowBox[{"w", "\[Rule]", " ", 
     RowBox[{
      RowBox[{"-", "5"}], "/", "6"}]}]}], ",", 
   RowBox[{"\[Psi]", "[", "lna", "]"}], ",", " ", "lna"}], "]"}]], "Input",
 CellChangeTimes->{{3.809678946615193*^9, 3.8096790693280373`*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"ededa87b-df42-4172-b536-294c90a50d8b"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"\[Psi]", "[", "lna", "]"}], "\[Rule]", 
    RowBox[{
     RowBox[{
      TemplateBox[{"1"},
       "C"], " ", 
      RowBox[{"Hypergeometric2F1", "[", 
       RowBox[{
        FractionBox["2", "5"], ",", 
        FractionBox["11", "10"], ",", "2", ",", 
        RowBox[{
         RowBox[{"-", 
          FractionBox["7", "3"]}], " ", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{
           RowBox[{"5", " ", "lna"}], "/", "2"}]]}]}], "]"}]}], "+", 
     RowBox[{
      TemplateBox[{"2"},
       "C"], " ", 
      RowBox[{"MeijerG", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["1", "10"]}], ",", 
            FractionBox["3", "5"]}], "}"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "1"}], ",", "0"}], "}"}], ",", 
          RowBox[{"{", "}"}]}], "}"}], ",", 
        RowBox[{
         RowBox[{"-", 
          FractionBox["7", "3"]}], " ", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{
           RowBox[{"5", " ", "lna"}], "/", "2"}]]}]}], "]"}]}]}]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{
