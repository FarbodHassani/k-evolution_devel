Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"numAmplitude", "=", "12"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"i", "=", "0"}], ",", 
     RowBox[{"i", "<", 
      RowBox[{"numAmplitude", "+", "1"}]}], ",", 
     RowBox[{
      RowBox[{"++", "i"}], ";", 
      RowBox[{
       RowBox[{"amplitude", "[", "i", "]"}], "=", " ", 
       RowBox[{
        RowBox[{"-", "4"}], "*", 
        RowBox[{
         SuperscriptBox["2", "i"], "/", "100"}]}]}], ";", 
      RowBox[{
       RowBox[{"nsol", "[", "i", "]"}], " ", "=", 
       RowBox[{"NDSolve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"Derivative", "[", 
                   RowBox[{"2", ",", "0"}], "]"}], "[", "p", "]"}], "[", 
                 RowBox[{"lna", ",", "r"}], "]"}], " ", "+", " ", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "-", 
                   RowBox[{"3", "w"}], "-", "dlnHdlna"}], ")"}], 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Derivative", "[", 
                    RowBox[{"1", ",", "0"}], "]"}], "[", "p", "]"}], "[", 
                  RowBox[{"lna", ",", "r"}], "]"}]}], "+", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"3", "w", 
                    RowBox[{"(", 
                    RowBox[{"dlnHdlna", "-", "1"}], ")"}]}], "-", 
                   RowBox[{"D", "[", 
                    RowBox[{"dlnHdlna", ",", "lna"}], "]"}]}], ")"}], 
                 RowBox[{"p", "[", 
                  RowBox[{"lna", ",", "r"}], "]"}]}], "+", 
                RowBox[{"3", "w", " ", 
                 RowBox[{"\[Psi]", "[", 
                  RowBox[{"lna", ",", "r"}], "]"}]}], "-", 
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"\[Psi]", "[", 
                   RowBox[{"lna", ",", "r"}], "]"}], ",", "lna"}], "]"}]}], 
               " ", "\[Equal]", 
               RowBox[{
                FractionBox[
                 RowBox[{" ", 
                  RowBox[{"2", "-", 
                   RowBox[{"3", "w"}]}]}], 
                 RowBox[{"2", 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"\[CapitalOmega]m", " ", 
                    RowBox[{"Exp", "[", 
                    RowBox[{"-", "lna"}], "]"}]}], "+", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"1", "-", "\[CapitalOmega]m"}], ")"}], 
                    RowBox[{"Exp", "[", 
                    RowBox[{
                    RowBox[{"-", 
                    RowBox[{"(", 
                    RowBox[{"1", "+", 
                    RowBox[{"3", "w"}]}], ")"}]}], "lna"}], "]"}]}]}], 
                   ")"}]}]], 
                SuperscriptBox[
                 RowBox[{
                  RowBox[{
                   RowBox[{"Derivative", "[", 
                    RowBox[{"0", ",", "1"}], "]"}], "[", "p", "]"}], "[", 
                  RowBox[{"lna", ",", "r"}], "]"}], "2"]}]}], ")"}], ",", 
             RowBox[{
              RowBox[{"p", "[", 
               RowBox[{
                RowBox[{"-", "8"}], ",", "r"}], "]"}], " ", "\[Equal]", " ", 
              "0"}], ",", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Derivative", "[", 
                 RowBox[{"1", ",", "0"}], "]"}], "[", "p", "]"}], "[", 
               RowBox[{
                RowBox[{"-", "8"}], ",", "r"}], "]"}], "\[Equal]", " ", 
              RowBox[{
               RowBox[{"\[Psi]", "[", 
                RowBox[{
                 RowBox[{"-", "8"}], ",", "r"}], "]"}], "/", "2"}]}]}], "}"}],
            "/.", 
           RowBox[{"amplitude", "\[Rule]", " ", 
            RowBox[{"amplitude", "[", "i", "]"}]}]}], "/.", 
          RowBox[{"k", "\[Rule]", " ", "20"}]}], ",", "p", ",", 
         RowBox[{"{", 
          RowBox[{"lna", ",", " ", 
           RowBox[{"-", "7.5"}], ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", 
           RowBox[{"-", "2"}], ",", "2"}], "}"}], ",", 
         RowBox[{"MaxStepSize", "\[Rule]", " ", "0.02"}]}], "]"}]}], ";"}]}], 
    "]"}]}]}]], "Input",
