     RowBox[{
      RowBox[{"2", "\[CapitalOmega]m"}], " ", "+", " ", 
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"1", "-", "\[CapitalOmega]m"}], ")"}], 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "3"}], "w", " ", "lna"}], "]"}]}]}]]}]}], 
  ";"}]], "Input",
